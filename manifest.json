{
    "manifest_version": 3,
    "name": "Chrome Window Manager",
    "version": "2.0.0",
    "description": "Advanced window management for Chrome with workspace support and window tiling - Built with React + TypeScript + Tailwind CSS",
    "permissions": [
        "tabs",
        "windows"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}