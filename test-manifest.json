{
    "manifest_version": 3,
    "name": "Chrome Window Manager",
    "version": "2.0.0",
    "description": "Advanced window management for Chrome",
    "permissions": [
        "storage",
        "tabs",
        "windows",
        "commands",
        "notifications",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "options_page": "options.html",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}